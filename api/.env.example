NODE_ENV=production
PORT=8080
TRUST_PROXY=1
LOG_LEVEL=info

MONGO_URI=mongodb://user:pass@127.0.0.1:27017/shitmguard?authSource=admin
SESSION_SECRET=replace-me
SESSION_NAME=shitm.sid
SESSION_MAX_AGE_MS=86400000
COOKIE_SECURE=true
CORS_ORIGIN=https://yourdomain.com,https://www.yourdomain.com

DISCORD_CLIENT_ID=
DISCORD_CLIENT_SECRET=
DISCORD_CALLBACK_URL=https://yourdomain.com/auth/discord/callback
POST_LOGIN_REDIRECT=https://yourdomain.com/console

TOKEN_MIN_TTL_SECONDS=30
TOKEN_TTL_SECONDS=60
TOKEN_MAX_TTL_SECONDS=90
TOKEN_HASH_SECRET=replace-me
CONNECT_ENABLED=true
CONNECT_BLOCK_REASON=connect_disabled
CONNECT_COOLDOWN_SECONDS=15
CONNECT_RATE_PER_MIN=3
# If Entry uses NAT/masquerade, set false (Origin sees Entry IP).
ENFORCE_IP_MATCH=true

ENTRY_PUBLIC_HOST=entry.yourdomain.com
ENTRY_PUBLIC_PORT=30120
ENTRY_ALLOWLIST_URL=http://10.66.0.2:9001/allowlist
ENTRY_ALLOWLIST_TOKEN=replace-me
ENTRY_ALLOWLIST_TIMEOUT_SECONDS=60
ENTRY_ALLOWLIST_MAX_TTL_SECONDS=90

FIVEM_VALIDATE_SECRET=replace-me
